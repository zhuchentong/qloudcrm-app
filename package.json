    "@ionic/angular-toolkit": "~1.5.1",
    "@ngxs/devtools-plugin": "^3.4.3",
    "@types/echarts": "^4.1.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.0.0",
    "codelyzer": "~4.5.0",
    "commitizen": "^3.1.1",

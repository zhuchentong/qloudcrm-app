    "cordova-android": "8.0.0",
    "cordova-ios": "5.0.1",
    "core-js": "^2.5.4",
    "echarts": "^4.2.1",
    "ionic2-calendar": "^0.5.2",
    "linqts": "^1.12.5",
    "ng-zorro-antd-mobile": "^0.12.5",
    "ngx-easy-table": "10.1.0",
    "ngx-echarts": "^4.2.1",
    "ngx-f2": "^1.0.0",
    "rxjs": "^6.5.2",
    "tslib": "^1.9.0",
    "typeface-montserrat": "^0.0.54",
    "zone.js": "~0.8.29"
